#parse("/velocity/main.vm")
#@mainMain()
<a href="$WebPath.getISSUE_CREATE()">Create issue</a>
<h1>All issues</h1>
<div class="row row-3">
    #foreach($issue in $issues)
    <div class="col">
        <a href="/issue/$issue.getId()">
            <strong>$issue.getTitle()</strong> <br>by $issue.getStatus() <br>$issue.getDate()
        </a>
    </div>
    #end
</div>
#end